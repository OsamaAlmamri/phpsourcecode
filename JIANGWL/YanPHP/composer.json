{
  "name": "weilongjiang/yanphp",
  "description": "The YanPHP Framework.",
  "keywords": ["framework", "yanphp"],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "phpunit/phpunit": "5.* || 6.*",
    "monolog/monolog": "1.*",
    "beberlei/assert": "2.*",
    "respect/validation": "1.*",
    "nikic/fast-route": "1.*",
    "illuminate/database": "5.*",
    "illuminate/events": "5.*",
    "aura/session": "2.*"
  },
  "authors": [
    {
      "name": "Weilong Jiang",
      "email": "william@jwlchina.cn"
    }
  ],
  "autoload": {
    "classmap": [
      "System",
      "Application",
      "tests"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
