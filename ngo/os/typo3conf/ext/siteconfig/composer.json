{
    "name": "jykj/siteconfig",
    "type": "typo3-cms-extension",
    "description": "\u914d\u7f6e\u7cfb\u7edf\u6587\u4ef6",
    "authors": [
        {
            "name": "yangshichang",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Jykj\\Siteconfig\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jykj\\Siteconfig\\Tests\\": "Tests"
        }
    },
    "replace": {
        "siteconfig": "self.version",
        "typo3-ter/siteconfig": "self.version"
    }
}