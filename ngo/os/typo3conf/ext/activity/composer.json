{
    "name": "jykj/activity",
    "type": "typo3-cms-extension",
    "description": "\u5fd7\u613f\u8005\u6d3b\u52a8,\u5fd7\u613f\u8005\u4fe1\u606f\u6536\u96c6,\u62a5\u540d,\u7b7e\u5230",
    "authors": [
        {
            "name": "yangshichang",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Jykj\\Activity\\": "Classes"
        }
    },
    "autoload-dev": {
