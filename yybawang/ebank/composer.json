{
    "name": "yybawang/ebank",
    "description": "电商类项目站内虚拟积分与聚合支付通用解决方案",
    "keywords": ["电商", "laravel","ebank","laravel-ebank","yybawang","积分系统","聚合支付","发红包"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.12",
        "ext-json": "*",
        "ext-curl": "*",
        "arcanedev/log-viewer": "v4.4.*",
        "barryvdh/laravel-cors": "^0.11.0",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "hhxsv5/laravel-s": "~3.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^0.11.0",
        "yansongda/laravel-pay": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Libraries/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
