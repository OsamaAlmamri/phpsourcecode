{
    "name": "opencart/opencart",
    "type": "project",
    "description": "OpenCart",
    "keywords": ["opencart", "ecommerce", "framework", "opensource"],
    "homepage": "http://www.opencart.com",
    "license": "GPL-3.0+",
    "config": {
        "vendor-dir": "./upload/vendor/",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.1.8",
        "ext-curl": "^7.1",
        "ext-gd": "^7.1",
        "ext-json": "*",
        "ext-openssl": "^7.1",
        "abraham/twitteroauth": "^1.1",
        "cosenary/instagram": "^2.3",
        "filp/whoops": "^2.3",
        "geoip2/geoip2": "^2.7",
        "illuminate/database": "5.8.*",
        "illuminate/support": "5.8.*",
        "kozz/emoji-regex": "^1.0",
        "leafo/scssphp": "0.0.12",
        "maximebf/debugbar": "^1.14",
        "nesbot/carbon": "2.*",
        "overtrue/socialite": "^2.0",
        "swiftmailer/swiftmailer": "5.*",
        "symfony/var-dumper": "4.*",
        "twig/twig": "1.*",
        "zoujingli/wechat-php-sdk": ">=1.3.10"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
