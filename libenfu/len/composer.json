{
  "name": "LEN PHP",
  "license": "MIT",
  "description": "PHP Framework",
  "type": "Framwork",
  "keywords": [
    "PHP",
    "PHP Framework"
  ],
  "authors": [
    {
      "name": "libenfu",
      "email": "i@91coder.org",
      "homepage": "https://gitee.com/libenfu/"
    }
  ],
  "require": {
    "php": ">= 5.3.0",
    "filp/whoops": "*"
  },
  "require-dev": {
    "predis/predis": "^1.1",
    "mongodb/mongodb": "1.1.2",
    "phpunit/phpunit": "^6.2",
    "geoip2/geoip2": "^2.6",
    "endroid/qrcode": "^2.3",
    "symfony/finder": "^3.3",
    "firebase/php-jwt": "^5.0",
    "eaglewu/swoole-ide-helper": "dev-master"
  },
  "autoload": {
    "files": [
      "Len/Common/functions.php",
      "Len/Logger.php",
      "Len/Dispatcher.php",
      "Len/Manager.php",
      "Len/Router.php",
      "Len/BaseModel.php",
      "Len/BaseControl.php",
      "Len/Core.php",
      "Len/Signal.php"
    ],
    "psr-4": {
      "Config\\": "Len/Config/",
      "DB\\": "Len/DB/",
      "Handler\\": "Len/Handler/",
      "Input\\": "Len/Input/",
      "Lock\\": "Len/Lock/",
      "Output\\": "Len/Output/",
      "Model\\": "app/models/",
      "Xml\\": "Len/Xml",
      "JWT\\": "Len/JWT",
      "Cache\\": "Len/Cache",
      "Utility\\":"Len/Utility"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
