{% extends 'SonataAdminBundle::standard_layout.html.twig' %}

{% block sonata_admin_content %}
    <div class="col-md-12">
        <div class="box box-primary">
            <div class="box-header">
                <h4 class="box-title">
                    处理提现
                </h4>
            </div>
            <div class="box-body">
                {{ form(form, {'attr': {'id': 'withdraw-form'}}) }}
            </div>
            <div class="box-footer">
                <button type="submit" form="withdraw-form" class="btn btn-primary">确认</button>
            </div>
        </div>
    </div>
    {{ sonata_block_render_event('sonata.admin.edit.form.bottom', { 'admin': admin, 'object': history }) }}
{% endblock %}