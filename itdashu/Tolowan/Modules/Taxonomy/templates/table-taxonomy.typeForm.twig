<table class="table table-striped table-bordered table-hover" id="sample-table-1">
    <thead>
        <tr>
            <th>机读名</th>
            <th>名称</th>
            <th>操作</th>
        </tr>
    </thead>
    <tbody>
        {% for key,value in data %}
        <tr>
            <td>{{ value['machine'] }}</td>
            <td>{{ value['name'] }}</td>
            <td>
                <a class="btn btn-xs btn-info" data-target="#main" href="{{ url(['for':'adminTermList','type':key,'page':1]) }}">添加术语</a>
                <a class="btn btn-xs btn-info" data-target="#right_handle" href="{{ url(['for':'adminConfigListEditor','id':id,'line':key]) }}">编辑</a>
                <a class="btn btn-xs btn-danger" data-target="#right_handle" href="{{ url(['for':'adminConfigListDelete','id':id,'line':key]) }}">删除</a>
                <a class="btn btn-xs btn-danger" data-target="#main" href="{{ url(['for':'adminTermList','type':key,'page':1]) }}">术语列表</a>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>
