<!DOCTYPE HTML>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=11,IE=10,IE=9,IE=8">
    <meta http-equiv="Cache-Control" content="no-siteapp">
    <meta http-equiv="X-Dns-Prefetch-Control" content="on">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="applicable-device" content="pc,mobile">
    <meta name="MobileOptimized" content="width"/>
    <meta name="HandheldFriendly" content="true"/>
    {% block head %}
    <title>{{ title }}-{{ coreConfig.name }}</title>
    <meta name="description" content="{% if description is defined %}{{ description }}{% else %}{{ coreConfig.indexDescription }}{% endif %}">
    <meta name="keywords" content="{% if keywords is defined %}{{ keywords }}{% else %}{{ title }}{% endif %}">
    <meta name="apple-mobile-web-app-title" content="{{ title }}">
    {% endblock %}
    <link rel="shortcut icon" href="/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=11,IE=10,IE=9,IE=8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta http-equiv="Cache-Control" content="no-siteapp">
    <meta name='robots' content='noindex,follow' />
    <link id='_bootstrap-css' media='all' href="//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <link media='all' id='_fontawesome-css' href="//cdn.bootcss.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
    <link rel='stylesheet' id='_main-css' href='http://cdn.itdashu.com/themes/dabbs/css/main.css' type='text/css' media='all' />
    <!--[if lt IE 9]>
    <script src="//cdn.bootcss.com/html5shiv/r29/html5.min.js"></script>
    <![endif]-->
</head>

<body{% block body_class %} class="nav_fixed single-page"{% endblock %}>
    <header class="header">
        <div class="container">
            <h1 class="logo">
            <a href="/" title="{{ coreConfig.indexTitle }}">
                <img src="/logo.png">{{ coreConfig.indexTitle }}</a></h1>
            <ul id="site-nav-ul" class="site-nav site-navbar">
                {% cache "mainmenu" 172800 %}{{ view.r(menuRender('main')) }}{% endcache %}
            </ul>


            <div class="m-icon">
                <i class="fa fa-search"></i>
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>

{% block body %}{% endblock %}

{% include 'Themes/Dabbs/templates/footer.twig' %}

<script type='text/javascript' src="//cdn.bootcss.com/jquery/3.1.0/jquery.min.js"></script>
<script type='text/javascript' src='//cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js'></script>
<script src="//cdn.bootcss.com/jquery_lazyload/1.9.7/jquery.lazyload.min.js"></script>
<script type='text/javascript' src="http://cdn.itdashu.com/themes/dabbs/js/main.js"></script>

{{ assets.output(null,null,'footer') }}
{% block bottom %}{% endblock %}
</body>
</html>