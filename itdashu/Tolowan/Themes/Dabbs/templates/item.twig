<div class="table-responsive">
	<table class="table table-striped table-bordered table-hover" id="sample-table-1">
		<tbody>
			{% for ddkey,ddValue in data %}
			<tr class="">
				<td>
					<H4>
						{% if ddValue['url'] is empty %}
					   {{ ddValue['name'] }}
						{% else %}
						<a href="{{ url(ddValue['url']) }}" hrefbox=".main-content" class="ajax-load-link">{{ ddValue['name'] }}</a>
					{% endif %}
					</H4>
					<P>{{ ddValue['description'] }}</P>
				</td>
			</tr>
			{% endfor %}
		</tbody>
	</table>
</div>