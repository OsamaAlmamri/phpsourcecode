<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>管理后台</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="//cdn.bootcss.com/font-awesome/4.6.3/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="//cdn.bootcss.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="http://cdn.itdashu.com/themes/AdminLTE/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="http://cdn.itdashu.com/themes/AdminLTE/dist/css/skins/_all-skins.min.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="//cdn.bootcss.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="//cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

    <header class="main-header">
        <!-- Logo -->
        <a href="{{ url(['for':'adminFrame']) }}" class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini"><b>后</b>台</span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"><b>管理</b>后台</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            <!-- /.search form -->
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle bell-red" data-toggle="dropdown">
                            <i class="fa fa-tasks"></i>
                            <span class="badge badge-important">0</span>
                        </a>
                        <ul class="dropdown-menu bell-list" id="bell-ul">
                            <!-- queue list -->
                                <li class="dropdown-header">
                                    <i class="fa fa-bell"></i>
                                    <b id="bell-num">0</b>条通知
                                </li>
                        </ul>
                    </li>
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="fa fa-search"></i>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li>
                                <!-- search form -->
                                {{ view.r(formRender('search.adminSearchForm')) }}
                            </li>
                        </ul>
                    </li>
                    <!-- User Account: style can be found in dropdown.less -->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img src="//cdn.itdashu.com/themes/AdminLTE/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
                            <span class="hidden-xs">{{ user.name }}</span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li><a href="/" target="_blank">访问前台首页</a></li>
                            <li>
                                <a href="{{ url(['for':'logout']) }}">退出</a>
                            </li>
                        </ul>
                    </li>
                    <!-- Control Sidebar Toggle Button -->
                    <li>
                        <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">


            <!-- sidebar menu: : style can be found in sidebar.less -->
            {{ view.r(adminMenu) }}
        </section>
        <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div id="main" class="content-wrapper" data="{{ url(['for':'adminIndex']) }}">

    </div>
    <!-- /.content-wrapper -->
    <footer class="main-footer">
        <div class="pull-right hidden-xs">
            <b>Version</b> 2.3.2
        </div>
        <strong><a href="http://git.oschina.net/itdashu/Tolowan">Tolowan</a>.</strong> <a target="_blank" href="http://shang.qq.com/wpa/qunwpa?idkey=0d7af0cd86e319424f9df2bb942c4ad124f587ea98e125631e57700f852fce5f">加入QQ交流群：574199144</a>
    </footer>

    <!-- Control Sidebar -->
    {% include "Themes/AdminLTE/templates/aside.twig" %}
    <!-- /.control-sidebar -->
    <!-- Add the sidebar's background. This div must be placed
         immediately after the control sidebar -->
    <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->
<link rel="stylesheet" href="http://cdn.itdashu.com/library/js/loadding-bar/css/loadingbar.css">
<link rel="stylesheet" href="http://cdn.itdashu.com/library/tabledrag/assets/jquery.tabledrag.css">
<link rel="stylesheet" href="http://cdn.itdashu.com/library/jQueryUI/jquery-ui.min.css">
<link rel="stylesheet" href="http://cdn.itdashu.com/library/date-time/datepicker.css">
<!-- jQuery 2.2.0 -->
<script src="//cdn.bootcss.com/jquery/2.2.4/jquery.js"></script>
<!-- date-range-picker -->
<script src="//cdn.bootcss.com/moment.js/2.11.1/moment.min.js"></script>
<script src="//cdn.bootcss.com/moment.js/2.11.1/locale/zh-cn.js"></script>


<!-- jQuery UI 1.11.4 -->
<script src="//cdn.itdashu.com/library/jquery.cookie/jquery.cookie.min.js"></script>
<script src="//cdn.itdashu.com/library/jQueryUI/jquery-ui.min.js"></script>
<script src="//cdn.itdashu.com/themes/AdminLTE/plugins/notice/notice.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script type="text/javascript" src="//cdn.itdashu.com/library/tabledrag/jquery.tabledrag.js"></script>
<script type="text/javascript" src="//cdn.itdashu.com/library/js/loadding-bar/js/jquery.loadingbar.js"></script>
<script>
    $.widget.bridge('uibutton', $.ui.button);
</script>
<!-- Bootstrap 3.3.5 -->
<script src="//cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

<script src="//cdn.bootcss.com/jquery.form/3.51/jquery.form.min.js"></script>
<script src="//cdn.bootcss.com/jQuery-slimScroll/1.3.8/jquery.slimscroll.min.js"></script>
<script type="text/javascript" src="http://cdn.itdashu.com/library/jquery-validate/jquery.validate.js"></script>
<script type="text/javascript" src="http://cdn.itdashu.com/library/jquery-validate/localization/messages_zh.min.js"></script>
<script src="http://cdn.itdashu.com/themes/AdminLTE/dist/js/demo.js"></script>
<!-- AdminLTE App -->
<script src="http://cdn.itdashu.com/themes/AdminLTE/dist/js/app.min.js"></script>
<script src="http://cdn.itdashu.com/themes/AdminLTE/dist/js/js.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="http://cdn.itdashu.com/themes/AdminLTE/dist/js/pages/dashboard.js"></script>
<!-- AdminLTE for demo purposes -->
<!-- AdminLTE for demo purposes -->
</body>
</html>
