/* 通行证卡 */
.card_id{height:160px;border-radius:20rpx;color: #fff;position:relative;overflow:hidden}
.card_id .card-face{width:80rpx;height:80rpx;position: absolute;top:30rpx;left:30rpx;border-radius:40rpx;overflow: hidden;border: 1px #fff solid;}
.card_id .card-title{position: absolute;top:30rpx;left:130rpx;line-height:40rpx}
.card_id .card-tr{position: absolute;top:15px;right:20px;z-index:8;}
.card_id .card-store{position: absolute;bottom:10px;left:10px;z-index:8;}
.card_id .card-price{position: absolute;bottom:10px;right:10px;z-index:8;}
.card_id .card-no{position: absolute;bottom:5px;right:20px;}
.card_id .card-open{position: absolute;top:70px;left:20px;right: 20px;text-align: center;}
.card_id .card-open .card-open-text{padding: 10px 0px;}
.card_id .card-bg{border-radius:0rpx 20rpx 20rpx 0px;width:114px;height:160px;position: absolute;top:0px;right:0px;z-index:5;background-image:url('data:image/png;base64,');background-repeat: no-repeat;background-position: top right;}
.card_id.blue{background-image:-webkit-gradient(linear,0 50%,100% 50%,from(#5498FE),to(#0C6AF3));color: #FFF}
.card_id.red{background-image:-webkit-gradient(linear,0 50%,100% 50%,from(#FE6862),to(#ED352E));color: #FFF!important}
.card_id.green{background-image:-webkit-gradient(linear,0 50%,100% 50%,from(#3BE5DB),to(#CBFA40));color: #FFF}
.card_id.purple{background-image:-webkit-gradient(linear,0 50%,100% 50%,from(#BC72FE),to(#8F21F1));color: #FFF}
.card_id.yellow{background-image:-webkit-gradient(linear,0 50%,100% 50%,from(#FFAA59),to(#FF820B));color: #FFF}
.card_id.gray{background-image:-webkit-gradient(linear,0 50%,100% 50%,from(#CFD8DC),to(#ECEFF1));color: #353535!important}